 {
  "manifest_version": 3,
  "name": "Ad Friend",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "./scripts/service-worker.js"
  }
}


